{
  "name": "xmoney-examples",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"./node_modules/.bin/tsx --watch server/index.ts\" \"vite\"",
    "server-start": "./node_modules/.bin/tsx --watch server/index.ts",
    "server-stop": "kill-port $(node -r dotenv/config -p \"process.env.SERVER_PORT || 3001\")",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "prettier --write src server",
    "check-format": "prettier --check src server"
  },
